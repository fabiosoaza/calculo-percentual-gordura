        android:layout_marginTop="8dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/textViewHeaderPesoExcesso"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewHeaderPesoExcesso"
        app:layout_constraintTop_toBottomOf="@+id/textViewHeaderPesoExcesso" />

    <TextView
        android:id="@+id/textViewResultadoClassificacao"
        style="@style/styleLabelNormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/textViewHeaderClassificacao"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textViewHeaderClassificacao"
        app:layout_constraintTop_toBottomOf="@+id/textViewHeaderClassificacao" />

    <TextView
        android:id="@+id/textViewResultadoMassaMagra"
        style="@style/styleLabelNormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/textViewHeaderMassaMagra"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textViewHeaderMassaMagra"
        app:layout_constraintTop_toBottomOf="@+id/textViewHeaderMassaMagra" />

    <TextView
        android:id="@+id/textViewResultadoPercentualMassaMagra"
        style="@style/styleLabelNormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/textViewHeaderPercentualMassaMagra"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewHeaderPercentualMassaMagra"
