        app:layout_constraintEnd_toEndOf="@+id/textViewHeaderDensidadeCorporal"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="@+id/textViewHeaderDensidadeCorporal"
        app:layout_constraintTop_toBottomOf="@+id/textViewHeaderDensidadeCorporal" />


    <TextView
        android:id="@+id/textViewHeaderMassaMagra"
